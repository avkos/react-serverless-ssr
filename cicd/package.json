{
  "name": "app",
  "version": "0.1.0",
  "license": "MIT",
  "bin": {
    "app": "bin/app.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "build:ssr": "cd ../app && yarn build:ssr",
    "build:app": "cd ../app && yarn build",
    "synth": "yarn cdk synth && sam local start-api -t ./cdk.out/AppStack.template.json",
    "start-local": "sam local start-api -t ./cdk.out/AppStack.template.json",
    "build-start" : "yarn build:ssr && yarn synth && yarn start-local",
    "deploy": "yarn build:app && yarn build:ssr && yarn cdk deploy"
  },
  "devDependencies": {
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "2.10.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "aws-cdk-lib": "2.10.0",
    "constructs": "^10.0.0",
    "source-map-support": "^0.5.16"
  }
}
